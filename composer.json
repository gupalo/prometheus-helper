{
    "name": "gupalo/prometheus-helper",
    "description": "Prometheus Helper",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\PrometheusHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\PrometheusHelper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "gupalo/json": "*",
        "promphp/prometheus_client_php": "^2.7",
        "symfony/http-foundation": "^5.2|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
