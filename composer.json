{
    "name": "gupalo/prometheus-helper",
    "description": "Prometheus Helper",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\PrometheusHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\PrometheusHelper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "gupalo/json": "*",
        "promphp/prometheus_client_php": "^1.0|^2.11",
        "symfony/http-foundation": "^3.0|^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.5|^9.5|^10.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    }
}
